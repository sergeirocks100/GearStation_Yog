<article class="display">
	<section>
		<span class="label">Power:</span>
		<div class="content">
			{{=helper.link(data.on? 'On' : 'Off', data.on? 'power' : 'close', {'power' : 1})}}
		</div>
	</section>
	<section>
		<span class="label">Output Pressure:</span>
		<div class="content">
			{{=helper.link('Set', 'pencil', {'set_press' : 'set'})}}
			{{=helper.link('Max', 'plus', {'set_press' : 'max'}, data.set_pressure == data.max_pressure ? 'disabled' : null)}}
			<span class="buttoninfo">{{=data.set_pressure}} kPa</span>
		</div>
	</section>
	<section>
		<span class="label">Node 1:</span>
		<div class="content">
			{{=helper.link('', 'fast-backward', {'node1_c' : '-0.1'}, null)}}
			{{=helper.link('', 'backward', {'node1_c' : '-0.01'}, null)}}
			{{=helper.link('', 'forward', {'node1_c' : '0.01'}, null)}}
			{{=helper.link('', 'fast-forward', {'node1_c' : '0.1'}, null)}}
			<span class="buttoninfo">{{=data.node1_concentration}}%</span>
		</div>
	</section>
	<section>
		<span class="label">Node 2:</span>
		<div class="content">
			{{=helper.link('', 'fast-backward', {'node2_c' : '-0.1'}, null)}}
			{{=helper.link('', 'backward', {'node2_c' : '-0.01'}, null)}}
			{{=helper.link('', 'forward', {'node2_c' : '0.01'}, null)}}
			{{=helper.link('', 'fast-forward', {'node2_c' : '0.1'}, null)}}
			<span class="buttoninfo">{{=data.node2_concentration}}%</span>
		</div>
	</section>
</article>
