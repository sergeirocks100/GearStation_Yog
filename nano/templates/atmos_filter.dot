<article class="display">
	<section>
		<span class="label">Power:</span>
		<div class="content">
			{{=helper.link(data.on? 'On' : 'Off', data.on ? 'power-off' : 'close', 'power')}}
		</div>
	</section>
	<section>
		<span class="label">Output Pressure:</span>
		<div class="content">
			{{=helper.link('Set', 'pencil', 'pressure', {'set': 'custom'})}}
			{{=helper.link('Max', 'plus', 'pressure', {'set': 'max'}, data.set_pressure == data.max_pressure ? 'disabled' : null)}}
			<span class="buttoninfo">{{=data.set_pressure}} kPa</span>
		</div>
	</section>
	<section>
		<span class="label">Filter:</span>
		<div class="content">
			{{=helper.link('Nothing', null, 'filter', {'mode': ""}, data.filter_type ==  -1 ? 'selected' : null)}}
			{{=helper.link('Plasma', null, 'filter', {'mode': "plasma"}, data.filter_type ==  0 ? 'selected' : null)}}
			{{=helper.link('O2', null, 'filter', {'mode': "o2"}, data.filter_type ==  1 ? 'selected' : null)}}
			{{=helper.link('N2', null, 'filter', {'mode': "n2"}, data.filter_type ==  2 ? 'selected' : null)}}
			{{=helper.link('CO2', null, 'filter', {'mode': "co2"}, data.filter_type ==  3 ? 'selected' : null)}}
			{{=helper.link('N2O', null, 'filter', {'mode': "c2o"}, data.filter_type ==  4 ? 'selected' : null)}}
		</div>
	</section>
</article>
